    private void Mass_ConvertActionPerformed(java.awt.event.ActionEvent evt) {                                             
     
        
        double m= 1000;
        double n = 1000000;
        double mass1 = Double.parseDouble(M_Value.getText());

        if ( Mass_ComboBox.getSelectedItem().equals("mg to g")){
            //mg divide by 1000
            double mg2g = mass1/m;
            String value=String.format("%.3f", mg2g);
             M_Result.setText(value);     
        }
            
          else if ( Mass_ComboBox.getSelectedItem().equals("mg to kg")){
            double mg2kg = mass1/1000000;
            String value = String.format("%.6f", mg2kg);
             M_Result.setText(value);
        }
        
        else if ( Mass_ComboBox.getSelectedItem().equals("g to mg")){
            double g2mg = mass1*m;
            String value = String.format("%.3f", g2mg);
             M_Result.setText(value); 
        }
        
         else if ( Mass_ComboBox.getSelectedItem().equals("g to kg")){
            double g2kg = mass1/m;
            String value = String.format("%.3f", g2kg);
             M_Result.setText(value); 
        }
          
        else if ( Mass_ComboBox.getSelectedItem().equals("kg to mg")){
            double kg2mg = mass1*n;
            String value = String.format("%.3f", kg2mg);
             M_Result.setText(value);
        }
        
        else {
            double kg2g = mass1*m;
            String value = String.format("%.3f", kg2g);
             M_Result.setText(value); 
        }
        
       
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
    }                                            

    private void TimeConvertActionPerformed(java.awt.event.ActionEvent evt) {                                            

 //TIME
        
        //double units refer to the method of conversion used for each possible outcome m-h, h-m, s-m etc
        //s2m(*0.0166667) s2h(*0.000277778) m2s(*60) m2h(*0.0166667) h2s(*3600) h2m(*60)
      
       double s = 60; 
       double h= 3600;
       double time = Double.parseDouble(T_Value.getText()); //mass is example use correlating unit



        if (TimeComboBox.getSelectedItem().equals("Seconds to Minutes")){
            //calculations here
            double S2M= time/s;

            String value = String.format("%.2f",S2M );

            T_Result.setText(value);     
        }

        else if (TimeComboBox.getSelectedItem().equals("Seconds to Hours")){

            double S2H = time/h;

            String value = String.format("%.6f", S2H);

           T_Result.setText(value); 
        }
        
        else if (TimeComboBox.getSelectedItem().equals("Minutes to Seconds")){

            double M2S = time*s;
            
            String value = String.format("%.2f", M2S);

            T_Result.setText(value); 
        }
        
        else if (TimeComboBox.getSelectedItem().equals("Minutes to Hours")){

            double M2H = time/s;

            String value = String.format("%.2f", M2H);

            T_Result.setText(value); 
        }
        
        else if (TimeComboBox.getSelectedItem().equals("Hours to Minutes")){

            double H2M = time*s;

            String value = String.format("%.2f", H2M);

            T_Result.setText(value); 

        }
        
        else {

            double H2S = time*h;

            String value = String.format("%.2f", H2S );

           T_Result.setText(value); 
        }














     
    }                                           

    private void DistanceConvertActionPerformed(java.awt.event.ActionEvent evt) {                                                

    //DISTANCE
   
   //cm2m(/100), cm2km(/1000), m2cm(*100), m2km(*1000), km2cm(*1000), km2m(*100)
   
   double dist = Double.parseDouble(DistValueInput.getText()); 
   double d1 = 100; 
   double d2= 10000; 
   double d3= 1000;
   
        if (DistComboBox.getSelectedItem().equals("centimeters to meters")){
            //calculations here
            double CM2M = dist*d1;
            String value = String.format("%.2f",CM2M );
            DistReturnedValue.setText(value);     
        }
        else if (DistComboBox.getSelectedItem().equals("centimeters to kilometers")){
            double CM2KM = dist/d2;
            String value = String.format("%.4f", CM2KM);
            DistReturnedValue.setText(value); 
        }
        else if (DistComboBox.getSelectedItem().equals("meters to centimeters")){
            double M2CM = dist*d1;
            String value = String.format("%.2f", M2CM);
            DistReturnedValue.setText(value); 
        }
        else if (DistComboBox.getSelectedItem().equals("meters to kilometers")){
            double M2KM = dist/d3;
            String value = String.format("%.3f", M2KM);
            DistReturnedValue.setText(value); 
        }
        
        else if (DistComboBox.getSelectedItem().equals("kilometers to centimeters")){
            double KM2CM = dist*d2;
            String value = String.format("%.2f", KM2CM );
            DistReturnedValue.setText(value); 
        }
        else if (DistComboBox.getSelectedItem().equals("kilometers to meters")){
            double KM2M = dist/d3;
            String value = String.format("%.3f", KM2M);
            DistReturnedValue.setText(value); 

        }


    }                                               

    private void TempConvertActionPerformed(java.awt.event.ActionEvent evt) {                                            
                 
        //convert to c - (°F − 32) × 5/9 = °C
        //convert to f - (°C × 9/5) + 32 = °F
        double tempvalue = Double.parseDouble(TEMP_Value.getText());
        if (TempComboBox.getSelectedItem().equals("Celsius to Farenheit"))
        {
            double C2F= (tempvalue-9/5)+32;
            String value = String.format("%.2f",C2F );
            TEMPRESULTTextField.setText(value);     
        }

        else if (TempComboBox.getSelectedItem().equals("Celsius to Kelvin"))
        {
            double C2K= (tempvalue+273.15);
            String value = String.format("%.2f",C2K );
            TEMPRESULTTextField.setText(value);        
        }        
        else if (TempComboBox.getSelectedItem().equals("Farenheit to Kelvin"))
        {
            double F2K= (tempvalue-32)*5/9 + 272.15;
            String value = String.format("%.2f",F2K );
            TEMPRESULTTextField.setText(value);     
        }   
        else if (TempComboBox.getSelectedItem().equals("Kelvin to Celsius"))
        {
            double K2C= (tempvalue-273.15);
            String value = String.format("%.2f",K2C );
            TEMPRESULTTextField.setText(value);     
        }         
        else if (TempComboBox.getSelectedItem().equals("Kelvin to Farenheit"))
        {
            double K2F= (tempvalue-273.15)*9/5 + 32;
            String value = String.format("%.2f",K2F );
            TEMPRESULTTextField.setText(value);        
        } 
        else {
            double F2C = (tempvalue-32)*5/9;
            String value = String.format("%.2f", F2C);
            TEMPRESULTTextField.setText(value); 
        }
    }                                           
                
